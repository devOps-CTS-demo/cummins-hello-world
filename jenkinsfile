pipeline{
    agents any
    stages{
           stage('compile code')
           {
               steps{
                   withMaven(maven: 'Maven3.6.3') {
                       sh 'mvn clean compile'
                   }
                   
               }
               
           }
           stage('testing')
           
           {
               steps{
                   withMaven(maven: 'Maven3.6.3')
                   sh 'mvn test'
                   
               }
           }
           
            stage('Deployment stage')
           
           {
               steps{
                   withMaven(maven: 'Maven3.6.3')
                   sh 'mvn deploy'
                   
               }
           }
        
        
    }
}